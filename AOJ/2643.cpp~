#include<bits/stdc++.h>
using namespace std;

int H,W,N;
char A[111][111];
char buf[1024];
int dx[]={0,1,0,-1};
int dy[]={-1,0,1,0};
char dc[]="NESW";
int x,y,d;
int gx,gy;

bool ischeckC(){
  int nx = x + dx[d], ny = y + dy[d];
  if( A[nx][ny] == '#' ) return true;
  return false;
}
bool ischeck(char c){
  if( 'T' == c ) return true;
  if( 'C' == c ) return ischeckC();
  int cd=-1;
  for(int i=0;i<4;i++){
    if( dc[i] == c ) cd = i;
  }
  if( d == cd ) return true;
  return false;
}


int cnt = 0;
int res = 0;
void move(char c){
  int nx,ny;
  if( c == '^' ) {
    nx = x + dx[d]; ny = y + dy[d];
  } else if( c == 'v' ){
    nx = x + dx[(d+2)%4]; ny = y + dy[(d+2)%4];
  } else if( c == '>' )
    d=(
  else
    
    
  if( A[nx][ny] != '#' ){
    x = nx; y = ny;
  }
  res++;
  cout << d << " " << x << " "<< y << " " <<res << endl;
  if( A[x][y] == 'g' ) throw(0);
}
void consume(char c){
  if( buf[cnt] == c ){
    cnt++;
  } else {
    cout << "cnt : " << cnt << " c : " << c << " really : " << buf[cnt] << endl;
    throw(0);
  }
}

void program(bool );
void sentence(bool );
bool calc(bool );
bool ifs(bool );
bool whiles(bool );
string term();

void program(bool fl){
  for(;;){
    sentence(fl);
    if( cnt == N ) return;
  }
}

void sentence(bool fl){
  bool ret = calc(fl);
  if( ret ) return;
  ret = ifs(fl);
  if( ret ) return;
  ret = whiles(fl);
}

bool ifs(bool fl){
  if( buf[cnt] == '[' ){
    cnt++;
    string t = term();
    bool f = false;
    if( t[0] == '~' ){
      f = ischeck(t[1]);
      f = !f;      
    } else {
      f = ischeck(t[0]);
    }
    program(f && fl);          
    consume(']');
    return true;
  }
  return false;
}

bool whiles(bool fl){
  if( buf[cnt] == '{' ){
    cnt++;
    int mt = cnt;
    while( 1 ){
      cnt = mt;
      string t = term();
      bool f = false;
      if( t[0] == '~' ){
        f = ischeck(t[1]);
        f = !f;      
      } else {
        f = ischeck(t[1]);
      }
      program(f && fl);          
      if( !f ) break;
    }
    consume('}');
    return true;
  }
  return false;
}

string term(){
  string ret;
  if( buf[cnt] == '~' )
    ret += buf[cnt++];
  ret += buf[cnt++];
  return ret;
}

bool calc(bool fl){
  cout << buf[cnt] << " "<< fl << endl;
  if( buf[cnt] == '^' ) {
    cnt++; if(fl) move(0);
  } else if( buf[cnt] == 'v' ){
    cnt++; if(fl) move(2);
  } else if( buf[cnt] == '<' ){
    cnt++; if(fl) move(3);
  } else if( buf[cnt] == '>' ){
    cnt++; if(fl) move(1);
  } else
    return false;
  return true;
}


int main(){
  cin >> H >> W;
  for(int i=0;i<H;i++){
    for(int j=0;j<W;j++){
      cin >> A[j][i];
      if( A[j][i] == 's' ) {
        x = j; y = i; d = 0;
      }
      if( A[j][i] == 'g' ){
        gx = j; gy = i;
      }
    }
  }
  cin >> buf;
  N = strlen(buf);
  bool f =false;
  cnt = 0;
  try{
    program(true);
  } catch(...) {
      cout << res << endl;
      f = true;
  }
  if( !f ) cout << -1 << endl;
}
